{
  "manifest_version": 3,
  "name": "Mäti",
  "version": "1.3.0",
  "description": "Save links, notes, and ideas directly to your Mäti account.",
  "permissions": [
    "activeTab",
    "contextMenus",
    "notifications",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
